    "print(classification_report(y_cv,y_pred_l))\n",
    "print('************************')\n",
    "cnf_matrix_l = metrics.confusion_matrix(y_cv, y_pred_l)\n",
    "tn, fp, fn, tp = metrics.confusion_matrix(y_cv, y_pred_l).ravel()\n",
